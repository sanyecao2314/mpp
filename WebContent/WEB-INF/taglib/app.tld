	<description>tag created by david</description>
	<display-name>davidtag</display-name>
	<tlib-version>1.0</tlib-version>
	<short-name>dtg</short-name>
	<uri>http://www.citsamex.com/taglib/app</uri>
	
	<tag>
		<description>select controller</description>
		<name>select</name>
		<tag-class>com.citsamex.app.tag.SelectTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>id</description>

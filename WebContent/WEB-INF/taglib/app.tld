			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>

		<attribute>
			<description>input</description>
			<name>input</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<description>css, table css</description>
			<name>css</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>

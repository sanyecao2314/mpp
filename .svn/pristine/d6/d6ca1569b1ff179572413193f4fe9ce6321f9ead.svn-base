<script type="text/javascript" >
	$(function() {
		$("#clause-form").hide();
	});
	
</script>
<div id="clause-form" title="Form - Clause" style="display:none">
		<div
			id="clause-form_message"> <input type="hidden"
			name="clause.id">
			<table cellpadding="5" cellspacing="0">
				<tr>
					<td colspan="2"><div id="clause-form-message"></div></td>
					
				</tr>
				<tr>
					<td width="30">Clause No:</td>
					<td><input type="text"
						id="clause-catalog" size="5" maxlength="5"> - <input type="text"
						id="clause-no" size="5" maxlength="5"></td>
				</tr>

				<tr>
					<td colspan="2">Content:</td>
				</tr>

				<tr>
					<td colspan="2"><textarea id="clause-content" cols="70" rows="7"></textarea>
					</td>
				</tr>

				<tr>
					<td colspan="2"><input type="radio" name="clause-status" value="ACTIVE" checked="checked"/>ACTIVE
						<input type="radio" name="clause-status" value="EXPIRED" />EXPIRED</td>
				</tr>
			</table> </div>
	
</div>





<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.citsamex.service.db.MemberPo" table="T_MEMBER">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="prodType" column="PROD_TYPE"></property>
		<property name="memberNo" column="MEMBER_NO"></property>
		<property name="memberName" column="MEMBER_NAME"></property>
		<property name="memberCode" column="MEMBER_CODE"></property>
		<property name="memberIssue" column="MEMBER_ISSUE"></property>
		<property name="memberExpire" column="MEMBER_EXPIRE"></property>
		<property name="remark" column="REMARK"></property>
		<property name="custno" column="CUSTNO"></property>
		<property name="relationId" column="RELATION_ID"></property>
		<property name="createuserno" column="CREATEUSERNO" update="false"></property>
		<property name="createdatetime" column="CREATEDATETIME" update="false"></property>
		<property name="updateuserno" column="UPDATEUSERNO"></property>
		<property name="updatedatetime" column="UPDATEDATETIME"></property>
	</class>

	<class name="com.citsamex.service.db.MemberHisPo" table="T_MEMBER_HIS">
		<id name="seq" type="java.lang.Long">
			<column name="seq" />
			<generator class="native" />
		</id>
		<property name="id" column="ID"></property>
		<property name="prodType" column="PROD_TYPE"></property>
		<property name="memberNo" column="MEMBER_NO"></property>
		<property name="memberName" column="MEMBER_NAME"></property>
		<property name="memberCode" column="MEMBER_CODE"></property>
		<property name="memberIssue" column="MEMBER_ISSUE"></property>
		<property name="memberExpire" column="MEMBER_EXPIRE"></property>
		<property name="remark" column="REMARK"></property>
		<property name="custno" column="CUSTNO"></property>
		<property name="relationId" column="RELATION_ID"></property>
		<property name="createuserno" column="CREATEUSERNO" update="false"></property>
		<property name="createdatetime" column="CREATEDATETIME" update="false"></property>
		<property name="updateuserno" column="UPDATEUSERNO"></property>
		<property name="updatedatetime" column="UPDATEDATETIME"></property>
		<property name="logdatetime" column="LOGDATETIME"></property>
	</class>

</hibernate-mapping>
